{
  "name": "kelseyleaphotography",
  "version": "0.0.1",
  "scripts": {
    "start": "svelte-kit dev",
    "build": "svelte-kit build",
    "build:prod": "(sleep 30 && svelte-kit build)",
    "postbuild": "svelte-sitemap --domain https://kelseyleaphotography.com -o .vercel_build_output/static",
    "preview": "svelte-kit preview",
    "build:sanity": "cd studio && npm run build",
    "start:sanity": "cd studio && npm run start",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
  },
  "devDependencies": {
    "@portabletext/svelte": "^1.0.1",
    "@sveltejs/adapter-static": "^1.0.0-next.42",
    "@sveltejs/adapter-vercel": "^1.0.0-next.73",
    "@sveltejs/kit": "^1.0.0-next.480",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.7.0",
    "svelte": "^3.50.1",
    "svelte-sitemap": "^2.5.1"
  },
  "type": "module",
  "dependencies": {
    "@sanity/client": "^3.4.1",
    "@sanity/image-url": "^1.0.1"
  }
}
